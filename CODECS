inter/AMBIGUOUS-PAD

inter/BIG5-DEFRAG

inter/CHEWING
	> echo abc測試xyz|bsdconv utf-8:chewing:utf-8
	abcㄘㄜˋㄕˋxyz

inter/CNS11643

inter/FROM_ALIAS

inter/FULL
	> echo testTEST1234 | bsdconv utf-8:full:utf-8
	ｔｅｓｔＴＥＳＴ１２３４

inter/HALF
	> echo ｔｅｓｔＴＥＳＴ１２３４ | bsdconv utf-8:half:utf-8
	testTEST1234

inter/HAN_PINYIN
	> echo 測試 | bsdconv utf-8:chewing:han_pinyin:utf-8
	ce4[sh]4

inter/INTER_ALIAS

inter/JP_PINYIN

inter/LOWER
	> echo testTEST | bsdconv utf-8:lower:utf-8
	testtest

inter/MAC

inter/NL2BR

inter/TO_ALIAS

inter/UNICODE

inter/UNIX

inter/UPPER
	> echo testTEST | bsdconv utf-8:upper:utf-8
	TESTTEST

inter/WIN

inter/UPSIDEDOWN
	> echo FUNNY | bsdconv utf-8:upsidedown:utf-8
	Ⅎ∩ᴎᴎ⅄

inter/ZHCN
	> echo 測試 | bsdconv utf-8:zhcn:utf-8
	测试

inter/ZHTW
	> echo 測試之后|bsdconv utf-8:zhtw:utf-8
	測試之后

inter/ZHTW_WORDS
	> echo 測試之后 | bsdconv utf-8:zhtw:zhtw_words:utf-8
	測試之後

inter/ZH_COMP
inter/ZH_DECOMP
	#used in https://github.com/buganini/chicomp

inter/AMBIGUOUS-UNPAD
	#used in https://github.com/buganini/bug5

inter/WIDTH

inter/SCORE

inter/WHITESPACE-DERAIL
inter/WHITESPACE-RERAIL
	> echo 之 后 | bsdconv utf-8:zhtw:zhtw_words:utf-8
	之 后
	> echo 之 后 | bsdconv utf-8:whitespace-derail:zhtw:zhtw_words:whitespace-rerail:utf-8
	之 後

inter/SPLIT
	> echo 測試|bsdconv utf-8:bsdconv
	016E2C018A66010A
	> echo 測試 | bsdconv utf-8:split:bsdconv_keyword,bsdconv
	016E2C,018A66,010A,

inter/SCORE_TRAIN

to/3F

to/ANSI-CONTROL
	#used in https://github.com/buganini/bug5

to/ASCII-ESCAPED-UNICODE

to/ASCII-HEX-NUMERIC-HTML-ENTITY

to/ASCII-HTML-CNS11643-IMG

to/ASCII-HTML-INFO
	> echo 測 | bsdconv utf-8:ascii-html-info
	<a href="http://www.cns11643.gov.tw/AIDB/query_general_view.do?page=1&code=6041"><img src="http://www.cns11643.gov.tw/AIDB/png.do?page=1&code=6041" /></a><a href="http://www.fileformat.info/info/unicode/char/0A/index.htm"><img class="unicode_img" src="http://www.unicode.org/cgi-bin/refglyph?24-A" /></a>
	
to/ASCII-HTML-UNICODE-IMG
	> echo 測 | bsdconv utf-8:ascii-html-unicode-img
	<img class="unicode_img" src="http://www.unicode.org/cgi-bin/refglyph?24-6E2C" /><img class="unicode_img" src="http://www.unicode.org/cgi-bin/refglyph?24-A" />

to/ASCII-NAMED-HTML-ENTITY
	> echo Ç | bsdconv utf-8:ascii-named-html-entity
	&Ccedil;

to/ASCII
	#just ascii

to/BIG5-5C
	> echo 功能|bsdconv utf-8:big5-5c,big5|hexdump -C
	00000000  a5 5c 5c af e0 0a                                 |.\\...|

to/BSDCONV
	> echo 測 | bsdconv utf-8:bsdconv
	016E2C010A

