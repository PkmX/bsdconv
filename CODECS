to/BSDCONV_LOG

to/BSDCONV_RAW
	> echo 測 | bsdconv utf-8:bsdconv_raw|hexdump -C
	00000000  01 6e 2c 01 0a                                    |.n,..|

to/BSDCONV_STDOUT
	#foreground debug tool
	> echo 測試 | bsdconv utf-8:bsdconv_stdout
	016E2C ( FREE )
	018A66 ( FREE )
	010A

